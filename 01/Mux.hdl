/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    And(a=s, b=b, out=anda)
    Not(in=s, out=nots)
    And(a=nots, b=a, out=andb)
    Or(a=anda, b=andb, out=out)
}